					<string>^You look at (?:the|an?) (.*) (\S+) (?:is|are) wielding.$</string>
				</regexCodeList>
				<regexCodePropertyList>
					<integer>1</integer>
				</regexCodePropertyList>
			</Trigger>
			<Trigger isActive="no" isFolder="no" isTempTrigger="no" isMultiline="no" isPerlSlashGOption="no" isColorizerTrigger="no" isFilterTrigger="no" isSoundTrigger="no" isColorTrigger="no" isColorTriggerFg="no" isColorTriggerBg="no">
				<name>GroupWeaponCondition</name>
				<script>cecho("&lt;yellow&gt;   [Group Weapon Condition Detection: "..matches[2].."]")
disableTrigger("GroupWeaponCondition")
if matches[2] ~= 'perfect' and matches[2] ~= 'excellent' and matches[2] ~= 'very good' then
	--send("rem "..My.weapon)
	--send("gt [BOT] my weapon's very worn! I have removed it.")
  if matches[2] == 'worn' then
    send("gstatus [WORNING] "..WeaponGroupMemeber.."'s "..WeaponGroupWeapon.." is " .. matches[2])
  elseif matches[2] == 'damaged' or matches[2] == 'worthless' or matches[2] == 'very worn' then
    send("gstatus [**ALERT**] "..WeaponGroupMemeber.."'s "..WeaponGroupWeapon.." is " .. matches[2])
    send("gstatus [**ALERT**] critical weapon condition! stopping the bot! &amp; gstatus stop")
  else
    send("gstatus "..WeaponGroupMemeber.."'s "..WeaponGroupWeapon.." is " .. matches[2])
  end
end</script>
