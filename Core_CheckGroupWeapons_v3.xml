				<name>looking at someone's weapon</name>
				<script>WeaponGroupMemeber = matches[3]
if WeaponGroupMemeber == "you" then WeaponGroupMemeber = gmcp.Char.Score.name end
WeaponGroupWeapon = matches[2]
enableTrigger("GroupWeaponCondition")</script>
				<triggerType>0</triggerType>
